                assertFalse(driver.getPageSource().contains("페이지 주소가 잘못 입력되었거나, 이미 삭제된 페이지입니다."));
                assertTrue(driver.getPageSource().contains("캐릭터 통합능력치랭킹 산정 기준"));
                System.out.println("랭킹 > 캐릭터랭킹 > 히페리온 > 리스트 : Pass");
            	Thread.sleep(1000);
            	js.executeScript("$('input[name=keyword]').val('123123')");
            	js.executeScript("$('.input-append > button').click();");
            	Thread.sleep(1000);
            	assertTrue(driver.getPageSource().contains("통합능력치 랭킹 내역이 없습니다."));
            	System.out.println("랭킹 > 캐릭터랭킹 > 히페리온 > 검색 : Pass");
        	}
    }
