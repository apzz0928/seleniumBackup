    	@Test
        public void F_ranking_f_libaration() throws Exception {
        	js.executeScript("$('.list-ranking > .liberation > a')[0].click();");
    		for(int i=1; i<=4; i++ ){
            	js.executeScript("$('.num-list > a')[" + (i-1) + "].click();");
                Thread.sleep(1000);
                assertFalse(driver.getPageSource().contains("페이지 주소가 잘못 입력되었거나, 이미 삭제된 페이지입니다."));
                assertTrue(driver.getPageSource().contains("시간을 기준으로 선정됩니다."));
                System.out.println("랭킹 > 해방랭킹 > 미리어드 > " + i + "인 랭킹 리스트 : Pass");
            	js.executeScript("$('input[name=keyword]').val('asdasd')");
            	Thread.sleep(1000);
            	js.executeScript("$('.input-append > button').click();");
            	Thread.sleep(1000);
                assertFalse(driver.getPageSource().contains("페이지 주소가 잘못 입력되었거나, 이미 삭제된 페이지입니다."));
            	System.out.println("랭킹 > 해방랭킹 > 미리어드 > " + i + "인 랭킹 리스트 검색 : Pass");
            	Thread.sleep(1000);
    		}
        	if(baseUrl.equals(liveUrl)){
        		js.executeScript("$('.ranking-server-tab > ul > li > a')[1].click();");
        		for(int i=1; i<=4; i++ ){
                	js.executeScript("$('.num-list > a')[" + (i-1) + "].click();");
                    Thread.sleep(1000);
                    assertFalse(driver.getPageSource().contains("페이지 주소가 잘못 입력되었거나, 이미 삭제된 페이지입니다."));
                    assertTrue(driver.getPageSource().contains("시간을 기준으로 선정됩니다."));
                    System.out.println("랭킹 > 해방랭킹 > 히페리온 > " + i + "인 랭킹 리스트 : Pass");
                	js.executeScript("$('input[name=keyword]').val('asdasd')");
                	Thread.sleep(1000);
                	js.executeScript("$('.input-append > button').click();");
                	Thread.sleep(1000);
                    assertFalse(driver.getPageSource().contains("페이지 주소가 잘못 입력되었거나, 이미 삭제된 페이지입니다."));
                	System.out.println("랭킹 > 해방랭킹 > 히페리온 > " + i + "인 랭킹 리스트 검색 : Pass");
                	Thread.sleep(1000);
        		}
        	}
    }
    
    @AfterClass
    public static void tearDown() throws Exception {
    	Thread.sleep(2000);
    	driver.close();  //driver.quit() 사용시 오류나는 경우 있음
        String verificationErrorString = verificationErrors.toString();
        if (!"".equals(verificationErrorString)) {
            fail(verificationErrorString);
        }
    }
}
